{
  "name": "discord-kim-bot",
  "version": "2.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "prepare": "node -e \"if (process.env.NODE_ENV !== 'production') { require('husky').install() }\"",
    "build": "tsc",
    "start": "pnpm debug:show-env && node index.js",
    "dev": "nodemon",
    "debug": "ts-node ./src/index.ts",
    "lint": "eslint src -c .eslintrc.cjs --ext .ts --fix",
    "commit": "cz",
    "commands": "NODE_ENV=development ts-node ./src/deploy-commands.ts",
    "debug:show-env": "node -e \"console.log(process.env)\""
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@changesets/changelog-github": "0.5.0",
    "@changesets/cli": "2.27.1",
    "@commitlint/cli": "18.4.4",
    "@commitlint/config-conventional": "18.4.4",
    "@commitlint/cz-commitlint": "18.4.4",
    "@types/node": "20.11.5",
    "@typescript-eslint/eslint-plugin": "6.19.0",
    "@typescript-eslint/parser": "6.19.0",
    "commitizen": "4.3.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "16.3.1",
    "husky": "^8.0.3",
    "nodemon": "3.0.3",
    "prettier": "3.2.4",
    "ts-node": "10.9.2",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@discordjs/builders": "1.7.0",
    "discord.js": "14.14.1",
    "mongoose": "8.1.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": "20.11.0"
  }
}